{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/search",
      "dest": "/search.html"
    },
    {
      "src": "/search(.*)",
      "dest": "/search.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
